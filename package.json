        "react-dom": ">=16.9.0",
        "react-icons": "^4.8.0"
    },
    "peerDependencies": {
        "react": ">=16.9.0",
        "react-dom": ">=16.9.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@mdx-js/react": "^1.6.22",
        "@storybook/addon-actions": "^7.0.0-rc.8",
        "@storybook/addon-essentials": "^7.0.0-rc.8",
        "@storybook/addon-interactions": "^7.0.0-rc.8",
